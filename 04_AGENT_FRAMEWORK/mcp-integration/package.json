{
  "name": "@localbrain/mcp-integration",
  "version": "1.0.0",
  "description": "Automatic agent coordination system with MCP integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:unit": "vitest run __tests__/SessionAutoDetect.test.ts",
    "test:integration": "vitest run __tests__/integration.test.ts",
    "test:performance": "vitest run __tests__/performance.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "agent-coordination",
    "automatic-agent",
    "localbrain",
    "multi-agent"
  ],
  "author": "LocalBrain Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/dexie": "^1.3.32",
    "dexie": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
